{
  "name": "is-in-editor",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.12.3",
  "description": "Check if running in an editor",
  "keywords": [
    "check",
    "vscode",
    "ide",
    "jetbrains"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/is-in-editor#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/is-in-editor/issues"
  },
  "repository": "ntnyq/is-in-editor",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "std-env": "^3.7.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.2.0",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.8.7",
    "@vitest/coverage-v8": "^2.1.4",
    "bumpp": "^9.8.0",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.1",
    "pnpm": "^9.12.3",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "unbuild": "2.0.0",
    "vitest": "^2.1.4"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
