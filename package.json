{
  "name": "is-in-editor",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.14.0",
  "description": "Check if running in an editor",
  "keywords": [
    "check",
    "editor",
    "ide",
    "jetbrains",
    "neovim",
    "vim",
    "vscode"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/is-in-editor#readme",
  "repository": "ntnyq/is-in-editor",
  "bugs": {
    "url": "https://github.com/ntnyq/is-in-editor/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.2.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "std-env": "^3.9.0",
    "tsdown": "^0.13.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
